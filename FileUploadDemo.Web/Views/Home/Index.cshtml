@model IndexViewModel

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th>Image</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var image in Model.Images)
        {
            <tr>
                <td>@image.Id</td>
                <td>@image.Title</td>
                <td>
                    <img src="/uploads/@image.ImagePath" style="height:300px;" />
                </td>
            </tr>
        }
    </tbody>
</table>